class Node{
    int data;
    Node next;
    Node(int data){
        this.data = data;
    }
}
pulic class kunalLinkedList{
    Node head;
    public void insertToEnd(int x){
        Node n = new Node(x);
        if(head == null){
            head = n;
        }
        else{
            Node temp = head;
            while(temp != null){
                temp = temp.next;
            }
            temp.next = n;
        }
    }
    public void deleteB(){
        if(head == null){
            return;
        }
        else{
            head = head.next;
        }
    }
    public void deleteE(){
        if(head.next == null){
            return ;
        }
        else{
            if(head.next == null){
                head = null;
            }
            else{
                Node temp = head;
                while(temp.next.next != null){
                    temp = temp.next;
                }
                temp.next = null;
            }
        }
    }


    public static void main(String[] args) {
        kunalLinkedList obj = new kunalLinkedList();
        obj.insertToEnd(10);
        obj.insertToEnd(20);
        obj.insertToEnd(30);        
    }
}